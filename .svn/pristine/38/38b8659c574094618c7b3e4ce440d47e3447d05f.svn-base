/*scheme-detail.html*/
.scheme-detail-body .house-container{
    background-color: #F18044;
    padding: 30px 0 40px 0;
}

.scheme-detail-body .house-room-wrap .image{
    /*padding-left: 20px;*/
    width: 80px;
    height: 70px;
    margin-top: 10px;
    margin-left: 15px;
}
.scheme-detail-body .house-room-wrap .image:hover .big-image{
    display: block;
}
.scheme-detail-body .house-room-wrap .big-image{
    position: absolute;
    width: 500px;
    height: 500px;
    padding: 20px;
    background: #ffffff;
