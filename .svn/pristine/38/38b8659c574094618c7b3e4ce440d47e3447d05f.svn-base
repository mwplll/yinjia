    border-right: 1px solid #a0a0a0;
}
.nav-tabs.nav-justified > li p{
    line-height: 20px;
    font-size: 14px;
}
.nav-tabs.nav-justified > li a{
    font-size: 18px;
    width: 100%;
    height: 100%;
    display: block;
    line-height: 38px;
}
.nav-tabs.nav-justified > li.active >div{
    background: #F18044;
    border-color: #F18044;
}
.nav-tabs.nav-justified > li.active .left{
    border-right: 1px solid white;
}
.nav-tabs.nav-justified > li.active a,
.nav-tabs.nav-justified > li.active p{
    color: #ffffff;

}
