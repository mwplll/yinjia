    margin-top: 20px;
}
.scheme-detail-body .house-room-wrap .button{
    margin-right: 25px;
    margin-top: 12px;
}
.scheme-detail-body .house-room-wrap .button .addBtn{
    /*background: url('../images/btn-06.png') no-repeat;*/
