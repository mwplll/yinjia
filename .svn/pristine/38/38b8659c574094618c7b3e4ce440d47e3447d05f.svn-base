}

/*detail-list.html*/
.scheme-body .breadcrumb-wrap, .scheme-detail-body .breadcrumb-wrap {
     margin-bottom: 0;
}
.scheme-detail-body .house-room-wrap{
    /*padding: 20px 20px 0 20px;*/
    /*background: #aadd00;*/
    /*margin-bottom: 15px;*/
    padding: 0;
    color: #ffffff;
    height: 80px;
}
.scheme-detail-body .house-room-wrap .title{
    line-height: 30px;
    min-width:400px;
    padding: 15px 40px 10px 5px;
    /*height: 35px;*/

}
.scheme-detail-body .house-room-wrap .price{
    border-left: 1px solid #ffffff;
    padding-left: 10px;
    margin-left: 70px;
