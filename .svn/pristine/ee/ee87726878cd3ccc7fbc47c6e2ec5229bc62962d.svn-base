.user-body .userInfo-wrap{
    background-color: #ffffff;
    height: 70px;
    line-height: 70px;
    margin-top: 25px;
}
.user-body .userInfo-wrap .image-wrap{
    width: 70px;
    height: 70px;
}
.user-body .userInfo-wrap .name{
    font-size: 18px;
}
.user-body .left-wrap{
    width: 148px;
    background-color: #ffffff;
    font-size: 18px;
}
.user-body .menu-wrap li{
    line-height: 70px;
    width: 100%;
    height: 70px;
    text-align: center;
}
.user-body .menu-wrap li a{
    display: block;
    width: 100%;
    height: 100%;
}
.user-body .menu-wrap li a:hover,
.user-body .menu-wrap li a.active{
    color: #ff0000;
}
.user-body .right-wrap{
    width: 920px;
    /*background-color: #ffffff;*/
    min-height: 500px;
}

.car-empty .no-shop {
    font-size: 24px;
    color: #555;
    padding: 43px 0 23px 0;
}
.car-empty .else {
    font-size: 15px;
    padding-bottom: 64px;
}

.user-body .right-wrap .list-item{
    width: 100%;
    height: 195px;
    margin-bottom: 23px;
    background-color: #ffffff;
}

.user-body .right-wrap .list-item .left-wrap,
.user-body .right-wrap .list-item .image-wrap{
    width: 290px;
    height: 195px;
}
.user-body .right-wrap .list-item .info-wrap{
    padding: 10px 0 0 27px;
}
.user-body .right-wrap .list-item .info-wrap .head{
    line-height: 40px;
}

.user-body .right-wrap .list-item .info-wrap