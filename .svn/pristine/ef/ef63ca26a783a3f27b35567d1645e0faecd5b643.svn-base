<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title>首页-印家</title>
    <meta charset="utf-8">
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta name="description" content="zyc-2014-11-17">
    <meta name="author" content="zyc">
    <meta property="qc:admins" content="1536567724607535471676375" />
    <!-- Le styles -->
    <link rel="stylesheet" type="text/css" href="js/bootstrap/3.2.0/css/bootstrap.css">

    <!-- font-awesome styles -->
    <link rel="stylesheet" type="text/css" href="js/bootstrap/font-awesome/font-awesome.min.css">

    <!-- main styles -->
    <link rel="stylesheet" type="text/css" href="css/common.css">
    <link rel="stylesheet" type="text/css" href="css/main.css">
    <link rel="stylesheet" type="text/css" href="css/index.css">

    <!--[if lt IE 8]>
    <link href="js/bootstrap/Bootstrap-IE7Fix-master/css/bootstrap-ie7fix.css" rel="stylesheet">
    <link href="css/hack-ie7.css" rel="stylesheet">
    <![endif]-->

    <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
    <!--[if lt IE 9]>
    <script src="js/html5shiv/3.7.0/html5.js"></script>
    <script src="js/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body ms-controller="IndexController">
    <div class="body index-body">
        <!--header start-->
        <div ms-include-src="'share/views/header.html'" ms-controller="HeaderController"></div>
        <!--header end-->

        <div class="session page1 tlc"
             ms-controller="CarouselController">
            <div class="main-container" style="height: 348px;">
                <div ms-widget="carousel, carousel1, $opt1"></div>
            </div>
        </div>

        <div class="session page3" style="padding-top: 50px;">
            <div class="main-container clearfix">
                <div class="pull-left left-wrap">
                    <div class="building-wrap">
                        <img src="images/new/1234_03.jpg" class="onetwo">
                        <div class="text-wrap">
                            <p class="p1">新楼盘<br/><span>全</span>覆盖</p>
                            <p class="p2">中国美院<br/><span>设</span>计团队</p>
                            <p class="p3">一对一户型<br/><span>全</span>方案设计</p>
                            <p class="p4">一个户型<br/><span>三</span>套设计方案</p>
                        </div>
                        <div class="building">
                            <div class="clearfix">
                                <a class="image-wrap pull-left"
                                   style="width: 364px;height: 230px;">
                                    <img src="images/new/building_03.jpg" width="364" height="230">
                                </a>
                                <div class="pull-right"
                                     style="width: 245px;padding-top: 20px;
                                            text-align: center;font-size: 45px;line-height: 54px;">
                                    <p>全面覆盖<br/>新楼盘<br/><span>15</span>个城市<br/><span>85</span>个楼盘</p>
                                </div>
                            </div>
                            <div style="margin-top: 30px;" class="building-list">
                                <ul class="clearfix">
                                    <li class="pull-left"
                                        ms-class-yellow="$index%2==1"
                                        ms-repeat="buildingList">
                                        <a target="_blank"
                                           ms-click="buildClick(el)">{{el.name}}</a>
                                    </li>
                                </ul>
                            </div>
                            <div class="bottom-wrap">
                                <ul class="clearfix">
                                    <li class="pull-left">
                                        <p>找户型</p>
                                        <span>一对一精准户型</span>
                                    </li>
                                    <li class="pull-left right"></li>
                                    <li class="pull-left">
                                        <p>选方案</p>
                                        <span>多方案选择</span>
                                    </li>
                                    <li class="pull-left right"></li>
                                    <li class="pull-left">
                                        <p>出造价</p>
                                        <span>预算=决算</span>
                                    </li>
                                    <li class="pull-left right"></li>
                                    <li class="pull-left">
                                        <p>定材料</p>
                                        <span>自由挑选</span>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="schema-list clearfix">
                        <div class="schema-item" ms-repeat="schemaList"
                             ms-class-nomargin="$index%2==1">
                            <a class="image-wrap cursorHand grey"
                               ms-href="pageLevel + 'schema/detail.html?schema_id=' + el.id + '&house_id=' + el.houseType.id">
                                <img ms-src="el.thumbUrl" class="fill">
                            </a>
                            <div class="info-wrap clearfix">
                                <div class="pull-left">
                                    <h2 class="title text-overflow"
                                        ms-attr-title="el.name"
                                        style="width: 299px;">{{el.name}}</h2>
                                    <p><span class="designer">{{el.houseType.city}}</span> <span class="time">{{el.houseType.building}}</span></p>
                                </div>
                                <div class="pull-right" style="padding-top: 10px;">
                                    <a class="btn btn-default btn-yinjia"
                                       ms-href="pageLevel + 'schema/list.html'">查看全部</a>
                                </div>
                            </div>
                            <div class="icon-wrap clearfix">
                                <ul class="clearfix">
                                    <li class="zan pull-left">
                                        <div class="clearfix">
                                            <i></i>  <span>{{el.likeNum}}</span>
                                        </div>
                                    </li>
                                    <li class="pin pull-left">
                                        <div class="clearfix">
                                            <i></i>  <span>{{el.commentNum}}</span>
                                        </div>
                                    </li>
                                    <li class="view pull-left">
                                        <div class="clearfix">
                                            <i></i>  <span>{{el.viewNum}}</span>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="other-list pull-right">
                    <div class="other-item gonggao">
                        <div class="title">
                            <img src="images/newIndex/icon_06.png"> 最新公告
                            <a class="fa fa-angle-double-right pull-right mt1 cursorHand"
                               href="article/list.html"></a>
                        </div>
                        <div class="content">
                            <ul>
                                <li ms-repeat="articleList">
                                    <a class="info"
                                       ms-href="'article/detail.html?id='+el.id">
                                      <p class="">{{el.title}}    <span>{{new Date(el.modifyTime*1000)| date('yyyy-MM-dd')}}</span></p>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="other-item huoban">
                        <div class="title">
                            <img src="images/newIndex/icon_26.png"> 合作伙伴
                            <a class="fa fa-angle-double-right pull-right mt1 cursorHand"></a>
                        </div>
                        <div class="content">
                            <ul class="clearfix">
                                <li class="pull-left cursorHand"
                                    ms-repeat="huoban">
                                    <div class="pull-left left"></div>
                                    <div class="pull-left">
                                        GuestBook
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!--footer start-->
        <div ms-include-src="'share/views/footer.html'" ></div>
        <!--footer end-->

    </div>

    <!--script-->
    <script type="text/javascript" src="js/plugins/SmoothScroll.js"></script>
    <script type="text/javascript" src="js/ngprogress/nprogress.js"></script>
    <script type="text/javascript" src="js/avalon/avalon.1.3.5-min.js"></script>

    <script type="text/javascript" src="config/config.js"></script>
    <script type="text/javascript" src="share/controllers/HeaderController.js"></script>


    <script type="text/javascript">
        headerCtr.cur = 'index';
        var indexCtr = avalon.define({
            $id: 'IndexController',

            pageLevel: base,
            schemaList: [],
            buildingList: [],
            buildClick: function(el){
                var select = {
                    area: el.area,
                    areaId: el.areaId,
                    buildId: el.id,
                    building: el.name
                };
                require(["UtilController"], function(AjaxFunc){

                    AjaxFunc.setCookie("select", JSON.stringify(select), 3600 * 24 * 7, '/');
                    AjaxFunc.setCookie("city2", el.city, 3600 * 24 * 7, '/');
                    AjaxFunc.setCookie("cityId2", el.cityId, 3600 * 24 * 7, '/');

                    window.open("schema/list.html", '_blank');
                });
            },
            articleList: [],
            huoban: [1]
        });
        require(['CarouselPlugin', "UtilController"], function(_, AjaxFunc){
            AjaxFunc.getAction({
                url: Global_URL['getSchemaList'],
                data: {
                    "recommend[]": [1],
                    num: 10,
                    page: 1
                },
                callback: function(result){
                    avalon.each(result.data.schemaList, function(i, item){
                        item.thumbUrl = image_base + item.mainPic + THUMB_SIZE['schema'];
                        item.totalPrice = Number(Number(item.totalPrice).toFixed(2));
                    });
                    indexCtr.schemaList = result.data.schemaList;
                }
            });
            AjaxFunc.getAction({
                url: Global_URL['getBuildingListByRecommend'],
                callback: function(result){
                    indexCtr.buildingList = result.data.buildingList;
                }
            });
            AjaxFunc.getAction({
                url: Global_URL['articleList'],
                data:{
                    num: 10,
                    page: 1,
                    catId: 2
                },
                callback: function(result){
                    indexCtr.articleList = result.data.articleList;
                }
            });

            avalon.define({
                $id: 'CarouselController',
                $opt1: {
                    pictures: [
                        "images/new/banner1.jpg",
                        "images/new/banner2.jpg",
                        "images/new/banner3.jpg",
                        "images/new/banner4.jpg"
                    ],
                    adaptiveWidth : '100%',
                    adaptiveHeight : "100%",
                    alwaysShowArrow: true,
                    hoverStop: true,

                    timeout: 2000,
                    during: 300
                }
            });
            avalon.scan();
        });

    </script>
</body>
</html>