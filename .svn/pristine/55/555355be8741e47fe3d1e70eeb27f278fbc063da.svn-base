@charset "UTF-8";
/*
这是每个都组件都应该引用的部分
*/
.ui-carousel {
  position: relative;
  margin: 0 auto;
  overflow: hidden;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -moz-user-select: none;
  user-select: none; }
  .ui-carousel .ui-carousel-panel {
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    overflow: hidden; }
    .ui-carousel .ui-carousel-panel .ui-carousel-item {
      position: relative;
      height: 100%;
      display: -moz-inline-stack;
      display: inline-block;
      vertical-align: middle;
      *vertical-align: auto;
      zoom: 1;
      *display: inline;
      overflow: hidden;
      text-align: center;
      /*background-color: #f2f2f2;*/
    }
      .ui-carousel .ui-carousel-panel .ui-carousel-item img {
        position: absolute;
        /*top: 50%;*/
        /*left: 50%;*/
        /*width: 100%;*/
        /*height: 100%;*/
          max-width: 100%;
          max-height: 100%;
          bottom: 0;
          right: 0;
          margin: auto;
        /*transform: translate(-50%, -50%);*/
        /*position: static\0;*/
        /*transform: translate(0, 0) \0;*/
        top: 0;
        left: 0; }
  .ui-carousel .ui-carousel-selection-wrap {
    position: absolute;
    bottom: 4%;
    left: 50%; }
    .ui-carousel .ui-carousel-selection-wrap a {
      display: -moz-inline-stack;
      display: inline-block;
      vertical-align: middle;
      *vertical-align: auto;
      zoom: 1;
      *display: inline;
      width: 10px;
      height: 10px;
      margin: 0 5px;
      background-color: rgba(255, 255, 255, 0.6);
      *background-color: white;
      border-radius: 50%;
      cursor: pointer; }
    .ui-carousel .ui-carousel-selection-wrap .selected {
      background-color: #00a3c2; }
  .ui-carousel .arrow {
    position: absolute;
    top: 50%;
    width: 30px;
    height: 30px;
    line-height: 24px;
    margin: -15px 0 0 -15px;
    color: #fff;
    text-align: center;
    font-size: 28px;
    cursor: pointer; }
  .ui-carousel .prev {
    left: 7%; }
  .ui-carousel .after {
    left: 93%; }
