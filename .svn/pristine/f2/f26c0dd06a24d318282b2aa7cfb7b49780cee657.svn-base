.schema-body{
    background-color: #ffffff;

}
.schema-body .page1{
    position: relative;
    height: 270px;
    overflow: hidden;
}
.schema-body .page1 img{
    position: absolute;
    width: 1900px;
    height: 603px;
    left: 50%;
    margin-left: -950px;
}
.schema-body .page2 .main-container{
    text-align: center;
    padding: 30px 20px 90px 5px;
}

.schema-body .page2 ul li{
    margin-left: 15px;
    margin-bottom: 10px;
}
.schema-body .page2 ul li.first a{
    background-color: #323639;
    border-color: #323639;
    color: #ffffff;
    padding-left: 25px;
    padding-right: 25px;
}
.schema-body .page2 ul li.first a:hover{
    background-color: #323639;
    border-color: #323639;
    color: #ffffff;
}
.schema-body .page2 ul li a{
    background-color: #ffffff;
    border-color: #f2f2f2;
    color: #cdcdcd;
    line-height: 28px;
    padding: 0 25px;
}
.schema-body .page2 ul li.last a{
    padding: 0 12px;
}
.schema-body .page2 ul li a.active,
.schema-body .page2 ul li a:hover{
    background-color: #ff4a00;
    border-color: #ff4a00;
    color: #ffffff;
}

.schema-body .page3{
    padding-bottom: 160px;
}
.schema-body .schema-list-wrap ul li{
    width: 300px;
    color: #b5b5b5;
    font-size: 14px;
    background-color: #ffffff;
    margin-right: 50px;
    margin-bottom: 10px;
    overflow: hidden;
}
.schema-body .schema-list-wrap ul li:nth-child(3n){
    margin-right: 0;
}

.schema-body .schema-list-wrap ul li .image-wrap{
    height: 200px;
    width: 100%;
}

.schema-body .schema-list-wrap ul li .info-wrap{
    padding-left: 10px;
    padding-bottom: 20px;
}
.schema-body .schema-list-wrap ul li.active,
.schema-body .schema-list-wrap ul li:hover{
    background-color: #ff4a00;
    color: #ffffff;
}
.schema-body .schema-list-wrap ul li .image-wrap{
    -webkit-transition: -webkit-transform 1s cubic-bezier(.23, 1, .32, 1),
                        -webkit-filter 1s cubic-bezier(.23, 1, .32, 1),
                        filter 1s cubic-bezier(.23, 1, .32, 1);
    -moz-transition:    -moz-transform 1s cubic-bezier(.23, 1, .32, 1),
                        -webkit-filter 1s cubic-bezier(.23, 1, .32, 1),
                        filter 1s cubic-bezier(.23, 1, .32, 1);
    transition:         transform 1s cubic-bezier(.23, 1, .32, 1),
                        -webkit-filter 1s cubic-bezier(.23, 1, .32, 1),
                        filter 1s cubic-bezier(.23, 1, .32, 1);
}
.schema-body .schema-list-wrap ul li.active .image-wrap,
.schema-body .schema-list-wrap ul li:hover .image-wrap{
    -webkit-transform: scale(0.96);
    -moz-transform: scale(0.96);
    transform: scale(0.96);
    -webkit-filter: saturate(0.96);
}

.schema-body .more{
    background-color: #f2f2f2;
    cursor: pointer;
    cursor: hand;
    text-align: center;
    color: #989898;
    font-size: 16px;
    border-radius: 4px;
    border: 1px solid #f2f2f2;
    margin-top: 15px;
    line-height: 40px;
}