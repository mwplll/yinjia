<div class="ui-dropdown"
     ms-class="ui-state-disabled:!enable"
     ms-class-1="{{titleClass}}"
     ms-css-width="{{width}}"
     ms-class-2="ui-state-focus: focusClass"
     ms-click="_toggle"
     ms-blur="_blur"
     ms-hover="ui-state-hover"
     ms-keydown="_keydown"
     tabindex="0">
    <div class="ui-dropdown-source">
        <div class="ui-dropdown-input"
             ms-title="title"
             ms-css-width="titleWidth"
             id="title-MS_OPTION_ID">{{label|sanitize|html}}</div>
        <div class="ui-dropdown-icon-wrap">
            <i class="ui-dropdown-icon ui-icon ui-icon-angle-up"
               ms-if="toggle">&#xf106;</i>
            <i class="ui-dropdown-icon ui-icon ui-icon-angle-down"
               ms-if="!toggle">&#xf107;</i>
        </div>
    </div>
</div>
MS_OPTION_TEMPLATE
<div class="ui-dropdown"
     ms-class="ui-dropdown-menu:!multiple"
     ms-class-1="{{listClass}}"
     ms-css-width="{{listWidth}}"
     ms-mouseenter="_listenter"
     ms-mouseleave="_listleave"
     ms-click="_listClick"
     ms-visible="toggle||multiple">
    <div class="ui-dropdown-menu-inner"
         ms-css-width="menuWidth"
         ms-css-height="menuHeight"
         ms-widget="scrollbar,scrollbar-MS_OPTION_ID" id="menu-MS_OPTION_ID">
        <div class="ui-scrollbar-scroller"
             id="list-MS_OPTION_ID">
            <div ms-repeat="data" data-repeat-rendered="updateScrollbar" class="ui-dropdown-item"
                 ms-click-12="_select($index, $event)"
                 ms-title="el.title||el.label"
                 ms-hover="ui-state-hover: el.enable"
                 ms-class-1="ui-state-disabled:!el.enable"
                 ms-class-2="ui-state-active:isActive(el) "
                 ms-class-4="ui-dropdown-group:el.group"
                 ms-class-5="ui-dropdown-divider:el.group && !$first"
                 >{{el.label|sanitize|html}}</div>
        </div>
    </div>
</div>
