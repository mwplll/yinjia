@charset "UTF-8";
/*
这是每个都组件都应该引用的部分
*/
.ui-daterangepicker { width: 260px; font-size: 12px; line-height: 18px; }
.ui-daterangepicker .ui-btn { height: 26px; overflow: visible; _overflow-y: hidden; padding: 0 10px; -webkit-border-radius: 2px; -moz-border-radius: 2px; -ms-border-radius: 2px; -o-border-radius: 2px; border-radius: 2px; font-size: 12px; color: #333333; background-color: #f8f8f8; border: 1px solid #cccccc; cursor: pointer; }
.ui-daterangepicker .ui-btn:hover { box-shadow: 1px 1px 0 0 rgba(0, 0, 0, 0.1); background-color: #f8f8f8; border-color: #bbbbbb; color: #333333; }
.ui-daterangepicker .ui-btn-small { height: 22px; padding: 0 5px; font-size: 12px; }
.ui-daterangepicker .ui-btn-primary { background-color: #3775c0; border-color: #3775c0; color: white; }
.ui-daterangepicker .ui-btn-primary.ui-state-hover { background-color: #5a8fd0; border-color: #5a8fd0; color: white; }
.ui-daterangepicker .ui-daterangepicker-content { position: relative; z-index: 1000; zoom: 1; }
.ui-daterangepicker .ui-daterangepicker-content .ui-daterangepicker-quick-operation { font-size: 12px; border-bottom: 1px solid #f2f2f2; margin: -5px 0 10px -10px; padding-left: 10px; }
.ui-daterangepicker .ui-daterangepicker-content .ui-daterangepicker-quick-operation span { color: #3775c0; padding: 6px 10px 6px 0; display: -moz-inline-stack; display: inline-block; vertical-align: middle; *vertical-align: auto; zoom: 1; *display: inline; cursor: pointer; }
.ui-daterangepicker .ui-daterangepicker-content .ui-datepicker-groups { position: absolute; z-index: 1000; zoom: 1; padding: 9px 0 0 10px; border: 1px solid #cccccc; box-shadow: 2px 2px 3px 0 rgba(0, 0, 0, 0.1); font-size: 0; white-space: nowrap; background: #ffffff; }
.ui-daterangepicker .ui-daterangepicker-content .ui-datepicker-groups .ui-datepicker-group { position: relative; display: -moz-inline-stack; display: inline-block; vertical-align: middle; *vertical-align: auto; zoom: 1; *display: inline; }
.ui-daterangepicker .ui-daterangepicker-content .ui-datepicker-groups .ui-datepicker-group .ui-datepicker-content { position: static; box-shadow: 0 0 0 0 transparent; }
.ui-daterangepicker .ui-daterangepicker-content .ui-datepicker-groups .ui-calendarbox-footer-small { position: relative; margin: 10px 0 0 -10px; height: 22px; line-height: 22px; padding: 4px 0; _width: 466px; background: #f8f8f8; }
.ui-daterangepicker .ui-daterangepicker-content .ui-datepicker-groups .ui-calendarbox-footer-small .ui-calendarbox-btns { position: absolute; right: 10px; top: 4px; display: inline; font-size: 0; }
.ui-daterangepicker .ui-daterangepicker-content .ui-datepicker-groups .ui-calendarbox-footer-small .ui-calendarbox-btns .ui-btn { margin: 0 0 0 5px; }
.ui-daterangepicker .ui-daterangepicker-content .ui-datepicker-groups .ui-calendarbox-footer-small .ui-calendarbox-msg { padding-left: 10px; font-size: 12px; }
.ui-daterangepicker .ui-daterangepicker-content .ui-datepicker-groups .ui-datepicker-wrapper { position: static; padding: 0; border: 0; box-shadow: 0 0 0 0 transparent; font-size: 12px; }
.ui-daterangepicker .ui-datepicker-range-input { cursor: pointer; border: 1px solid #cccccc; background-color: #ffffff; }
.ui-daterangepicker .ui-datepicker-range-input .ui-datepicker-input { height: 18px; overflow: hidden; padding: 3px 21px 3px 6px; text-overflow: ellipsis; white-space: nowrap; }
.ui-daterangepicker .ui-datepicker-range-input .ui-datepicker-input.ui-datepicker-input-disabled { background: #ebebe4; border-color: #cccccc; }
.ui-daterangepicker .ui-datepicker-range-input.ui-state-hover { border: 1px solid #999; }
.ui-daterangepicker .ui-datepicker-range-input.ui-state-active { border-color: #3775c0; border-bottom-color: #fff; }
.ui-daterangepicker .ui-datepicker-icon-wrap { position: relative; display: block; height: 0; }
.ui-daterangepicker .ui-datepicker-icon-wrap .ui-datepicker-icon { position: absolute; right: 0; top: -21px; display: none; cursor: pointer; padding: 0 6px; text-align: center; text-indent: 0; color: #b5b5b5; }
.ui-daterangepicker .ui-datepicker-icon-wrap .ui-datepicker-icon.ui-icon-angle-down, .ui-daterangepicker .ui-datepicker-icon-wrap .ui-datepicker-icon.ui-icon-angle-up { display: block; }
